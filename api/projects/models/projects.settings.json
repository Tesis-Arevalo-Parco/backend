{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Proyectos",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "description": {
      "type": "text"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "projects"
    },
    "assets": {
      "via": "project",
      "collection": "assets"
    },
    "dependency": {
      "model": "dependencies",
      "via": "project"
    },
    "threat": {
      "collection": "threats",
      "via": "project"
    },
    "date_project": {
      "type": "date"
    },
    "security_manager": {
      "type": "string"
    },
    "code_project": {
      "type": "string"
    }
  }
}
